APP_NAME="YLA Umzug Staging"
APP_ENV=staging
APP_KEY=base64:STAGING_KEY_WILL_BE_GENERATED
APP_DEBUG=false
APP_URL=http://localhost:8001

LOG_CHANNEL=stack
